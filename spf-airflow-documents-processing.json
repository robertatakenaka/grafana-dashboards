          "rawSql": "select\n    dag_run,\n    count(*) as \"Quantidade\",\n    package_name as \"Pacote\",\n    --(\n    --    case\n    --        failed\n    --        when true then 'Falha'\n     --       when false then 'Sucesso'\n     --   end\n    --) as \"Status\"\n    file_name as \"File\", \n    failed as \"Status\"\n    --to_char(created_at, 'dd-MM-YYYY') as \"Data\"\nFROM\n    xml_documents\nWHERE\n    $__timeFilter(created_at)\n    and dag_run LIKE concat('%', $pacotes_extraidos_dag_run, '%')\n    AND pre_sync_dag_run LIKE concat('%', $pre_sync_dag_run_id, '%')\n    AND (\n          pid LIKE concat('%', nullif('$pid_v3', ''), '%') OR pid is null\n        )\n    AND (CASE\n                    WHEN ${show_only_documents_errors:singlequote} ILIKE 'Falha' THEN\n                      failed = TRUE\n                    WHEN ${show_only_documents_errors:singlequote} ILIKE 'Sem falha' THEN\n                      failed = False\n                    ELSE\n                     failed IN (False, True)\n                  END)\nGROUP BY\n    dag_run,\n    failed,\n    \"File\",\n    \"Pacote\"\nORDER BY \"Quantidade\" DESC\n",
